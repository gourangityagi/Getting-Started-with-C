#include <iostream>
using namespace std;

class cube
{// public is an access specifier
    public:
    double height = 1.0;
    double width = 1.0;
    double length = 1.0;
};

int main()
{
cube cube1;//object of class cube  
double vol = cube1.height*cube1.width*cube1.length;
cout<<"The volume of cube is:- "<<vol<<endl;
return 0;

}
