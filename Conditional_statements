#include<iostream>
using namespace std;

int main()
{
int num;
int result;
cout<<"Enter your marks:- ";
cin>>num; 
cout<<"Result:-"<< (result = num) <<endl;
if (result > 90) 
    cout<<"Your grade is O"<<endl;
else if (result >= 80)
    cout<<"Your grade is A+"<<endl;
else if (result >= 70)
    cout<<"Your grade is A"<<endl;
else if (result >= 60)
    cout<<"Your grade is B+"<<endl;
else if (result >= 50)
    cout<<"Your grade is B"<<endl;
else if (result >= 40)
    cout<<"Your grade is C"<<endl;
else
    cout<<"You Failed"<<endl;

 return 0;
}
