#include <iostream>
using namespace std;

void swap(int*,int*);

int main()
{
int a = 10,b = 5;
cout << "Before swapping." << endl;
cout << "a = " << a << ", b = " << b << endl;
swap(&a,&b);
cout << "\nAfter swapping." << endl;
cout << "a = " << a << ", b = " << b << endl;

return 0;
}
void swap (int*a, int*b)
{
    int temp;
    temp = *a;
    *a = *b;
    *b = temp;
    
}
